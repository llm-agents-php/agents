{
  "name": "llm-agents/agents",
  "description": "LLM Agents PHP SDK",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "ramsey/uuid": "^4.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3"
  },
  "autoload": {
    "psr-4": {
      "LLM\\Agents\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "LLM\\Agents\\Tests\\": "tests/src"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
